{
  "name": "wouxun-si",
  "description": "wouxun qwik website",
  "version": "0.0.14",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "optionalDependencies": {
    "@img/sharp-linux-x64": "^0.33.2"
  },
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "docker:build": "./deploy/build.sh",
    "docker:publish": "./deploy/publish.sh",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {},
  "devDependencies": {
    "@builder.io/qwik": "^1.4.3",
    "@builder.io/qwik-city": "^1.4.3",
    "@directus/sdk": "^14.0.0",
    "@medusajs/client-types": "^0.2.8",
    "@medusajs/medusa": "^1.20.1",
    "@medusajs/medusa-js": "^6.1.6",
    "@medusajs/types": "^1.11.11",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "@paypal/paypal-js": "^8.0.0",
    "@stripe/stripe-js": "^2.4.0",
    "@types/eslint": "^8.56.2",
    "@types/js-cookie": "^3.0.6",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@unpic/qwik": "^0.0.41",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.4.3",
    "insane": "^2.6.2",
    "js-cookie": "^3.0.5",
    "ky": "^1.2.0",
    "markdown-it": "^14.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "swiper": "^11.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "undici": "*",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "workbox-recipes": "^7.0.0"
  }
}
