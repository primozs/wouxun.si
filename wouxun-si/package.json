{
  "name": "wouxun-si",
  "description": "wouxun qwik website",
  "version": "0.0.10",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "docker:build": "./deploy/build.sh",
    "docker:publish": "./deploy/publish.sh",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {},
  "devDependencies": {
    "@builder.io/qwik": "^1.2.8",
    "@builder.io/qwik-city": "^1.2.8",
    "@directus/sdk": "^11.0.2",
    "@medusajs/client-types": "^0.2.4",
    "@medusajs/medusa": "^1.14.0",
    "@medusajs/medusa-js": "^6.0.3",
    "@medusajs/types": "^1.10.2",
    "@mobily/ts-belt": "4.0.0-rc.5",
    "@paypal/paypal-js": "^7.0.0",
    "@stripe/stripe-js": "^2.1.0",
    "@types/eslint": "^8.44.2",
    "@types/js-cookie": "^3.0.3",
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@unpic/qwik": "^0.0.25",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "eslint": "^8.47.0",
    "eslint-plugin-qwik": "^1.2.8",
    "insane": "^2.6.2",
    "js-cookie": "^3.0.5",
    "ky": "^0.33.3",
    "markdown-it": "^13.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "swiper": "^10.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "undici": "^5.23.0",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "workbox-recipes": "^7.0.0"
  }
}
